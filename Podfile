  pod 'LBTAComponents'
  pod 'SnapKit', '~> 4.0.0'
  pod 'SwiftIconFont'
  target 'baseline_iosTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'baseline_iosUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
